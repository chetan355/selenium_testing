package com.selenium.handson.StreamsAndLambda;

import java.util.*;

/**
 * Hello world!
 *
 */
public class Test1 
{
	
	public void filterList() {
		List<String> list = new ArrayList<String>();
		list.add("Adam");
		list.add("Amisha");
		list.add("Chetan");
		list.add("Bhavesh");
		int count = 0;
		for(int i = 0; i < list.size(); i++) {
			if(list.get(i).startsWith("A")) {
				count++;
			}
		}
		System.out.println(count);
	}
}
